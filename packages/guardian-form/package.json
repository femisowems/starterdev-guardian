{
  "name": "@starterdev/guardian-form",
  "version": "1.0.0",
  "description": "Enterprise React + TypeScript form framework for regulated environments",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./adapters": {
      "types": "./dist/adapters/index.d.ts",
      "import": "./dist/adapters/index.mjs",
      "require": "./dist/adapters/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/adapters/index.ts --format cjs,esm --dts --minify --clean",
    "test": "vitest run",
    "lint": "eslint src --ext .ts,.tsx"
  },
  "keywords": [
    "react",
    "form",
    "governance",
    "pii",
    "security",
    "compliance",
    "banking",
    "fintech"
  ],
  "author": "Antigravity",
  "license": "MIT",
  "peerDependencies": {
    "react": "^18.0.0 || ^19.0.0",
    "react-dom": "^18.0.0 || ^19.0.0",
    "zod": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "zod": {
      "optional": true
    }
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "jsdom": "^28.1.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "eslint": "^8.0.0",
    "eslint-plugin-react": "^7.33.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^2.0.0",
    "zod": "^3.23.0"
  }
}